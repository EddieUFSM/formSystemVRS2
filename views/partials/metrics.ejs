<script>
    var ResponseStart;
    $(window).ready(function () {
        ResponseStart = new Date().getTime();
    });

    $(window).on('beforeunload', function () {
        var ResponseEnd = new Date().getTime();
        var ResponseTime = (ResponseEnd - ResponseStart) / 1000;
    });        
</script>

